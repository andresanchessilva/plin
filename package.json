{
  "name": "plin",
  "version": "1.0.0",
  "description": "Express + Sequelize + TypeScript project with PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down",
    "migrate:create": "db-migrate create",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=unit",
    "test:integration": "jest --testPathPatterns=integration",
    "test:setup": "./scripts/setup-test-db-sql.sh",
    "test:cleanup": "./scripts/cleanup-test-db.sh",
    "test:reset": "./scripts/reset-test-db.sh",
    "test:real": "npm run test:reset && npm test -- --testPathPatterns='real.test.ts'",
    "test:real:individual": "./scripts/run-real-tests-individually.sh",
    "test:only-real": "./scripts/run-real-tests-individually.sh"
  },
  "keywords": [
    "express",
    "sequelize",
    "typescript",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cheerio": "^1.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.9.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^6.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "jest": "^30.1.3",
    "nock": "^14.0.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
